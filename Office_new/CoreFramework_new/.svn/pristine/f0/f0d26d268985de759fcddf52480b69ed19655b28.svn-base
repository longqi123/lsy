import UIKit.UILabel

extension UILabel {
	
	/// 查询中的各种UILabel
	///
	/// - Parameter text: 字符串 
	public convenience init(text: String) {
		self.init()
		self.text = text
		font = .normal(12)
		textAlignment = .left
	}
	
}
