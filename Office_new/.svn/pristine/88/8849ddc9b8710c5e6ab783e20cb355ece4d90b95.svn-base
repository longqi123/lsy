import UIKit
import CoreFramework

class MessageCenterCell: BaseTableViewCell {

    @IBOutlet weak var title: UILabel!
    @IBOutlet weak var detail: UILabel!
    @IBOutlet weak var time: UILabel!
    @IBOutlet weak var weidu: UILabel!
    @IBOutlet weak var photo: UILabel!
    
    override func awakeFromNib() {
        super.awakeFromNib()
        title.font = UIFont.H3
        title.textColor = UIColor.T2
        detail.font = UIFont.H6
        detail.textColor = UIColor.T3
        time.font = UIFont.H8
        time.textColor = UIColor.T4
        weidu.font = UIFont.H10
        weidu.textColor = UIColor.T6
    
        photo.textColor = UIColor.T6
        photo.font = UIFont.H2
    }
}
